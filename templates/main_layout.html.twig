    <title>{% block title %}Send and Get{% endblock %}</title>

    {% block stylesheets %}
        <link href="{{ asset('assets/css/bootstrap.min.css') }}" rel="stylesheet"/>
        <link href="{{ asset('assets/fonts/import.css') }}" rel="stylesheet"/>
