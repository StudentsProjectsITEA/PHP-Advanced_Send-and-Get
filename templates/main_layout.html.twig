<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>{% block title %}Send and Get{% endblock %}</title>

    {% block stylesheets %}

        {#{{ encore_entry_link_tags('app') }}#}
        <link href="{{ asset('assets/css/bootstrap.min.css') }}" rel="stylesheet"/>
        <link href="{{ asset('assets/fonts/import.css') }}" rel="stylesheet"/>
        <link href="{{ asset('assets/vendor/font-awesome/font-awesome.css') }}" rel="stylesheet"/>
        <link href="{{ asset('assets/css/global.css') }}" rel="stylesheet"/>
    {% endblock %}

</head>
<body class="page page-{% if page is defined and page.slug is defined%}{{ page.slug }}{% else %}main{% endif %}">

{% if includeHeader is defined %}

    {% include '_header.html.twig' %}

{% endif %}
<div class="content container-fluid">

    {% if page is defined %}

        {% include '_jumbotron.html.twig' %}

    {% endif %}

    {% if page.breadcrumbs is defined %}

        {% include '_breadcrumb.html.twig' %}

    {% endif %}

    {% block content %}

    {% endblock %}
</div>

{% include '_footer.html.twig' %}

{% block javascripts %}

    {#{{ encore_entry_script_tags('app') }}#}

    <script src="{{ asset('assets/js/jquery-3.3.1.min.js') }}"></script>
    <script src="{{ asset("assets/js/bootstrap.bundle.min.js") }}"></script>
{% endblock %}
</body>
</html>